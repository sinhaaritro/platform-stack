# Ansible inventory generated by OpenTofu
all:
  children:
%{ for group_name, hosts in inventory_groups ~}
    ${group_name}:
      hosts:
%{ for host_name, host_vars in hosts ~}
        ${host_name}:
          ansible_host: ${host_vars.ansible_host}
          ansible_user: ${host_vars.ansible_user}
%{ endfor ~}
%{ endfor ~}