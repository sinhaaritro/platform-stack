# Ansible inventory generated by OpenTofu
all:
  children:
%{ for group_name, group_data in inventory_data ~}
    ${group_name}:
%{ if length(group_data.vars) > 0 ~}
      vars:
%{ for var_key, var_val in group_data.vars ~}
        ${var_key}: "${var_val}"
%{ endfor ~}
%{ endif ~}
      hosts:
%{ for host_name, host_vars in group_data.hosts ~}
        ${host_name}:
%{ for key, value in host_vars ~}
          ${key}: ${jsonencode(value)}
%{ endfor ~}
%{ endfor ~}
%{ endfor ~}